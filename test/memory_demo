void testTask(void *parameter)
{
    while (1)
    {
        int a = 1;
        int b = [100];

        for (int i = 0; i < 100; i++)
        {
            b[i] = a + i;
        }
        S
    }
}

void setup()
{
    // Configrue Serial
    Serial.begin(115200);
    vTaskDelay(pdMS_TO_TICKS(1000));
    Serial.println("Starting memory demo test task...");
    xTaskCreatePinnedToCore(
        testTask,
        "Test Task",
        1024,
        NULL,
        1,
        NULL);
    vTaskDelete(NULL);
}

void loop()
{
    // Empty loop
}